{
  "name": "project-salesforce-cypress",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cypress open",
    "test": "node_modules/.bin/cypress run",
    "checkstyle": "node_modules/.bin/eslint cypress",
    "cy:run": "node ./scripts/cypress.js",
    "allTests-headless": "npx cypress run",
    "allTests-headed": "npx cypress run --headed",
    "allTests-chrome": "npx cypress run --browser chrome",
    "allTests-firefox": "npx cypress run --browser firefox",
    "allTests-edge": "npx cypress run --browser edge",
    "run-dashboard": "npx cypress run --record --key 541783f3-7a1a-4851-965f-109d067874f9",
    "run-dashboard-parallel": "npx cypress run --record --key 541783f3-7a1a-4851-965f-109d067874f9 --parallel",
    "allTests-browser": "npx cypress run --spec cypress/integration/*",
    "cypress-multi-browser": "npm run allTests-browser -- --browser electron --headed & npm run allTests-browser -- --browser edge --headed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cypress": "^8.1.0"
  },
  "devDependencies": {
    "cypress-cucumber-preprocessor": "^4.2.0",
    "eslint": "^7.31.0",
    "mocha": "^9.0.3",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "moment": "^2.29.1",
    "multiple-cucumber-html-reporter": "^1.18.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}
